<!DOCTYPE html>
<html lang="en">
   <head>
    {% include _head.erb %}
   </head>
   <body id="post">
       {% include _header.erb %}
       <article class="page-header">
           <div class="center blog">
               <h1>{{ page.title }}</h1>
               {% if page.subtitle %}<h2>{{ page.subtitle }}</h2>{% endif %}
           </div>
       </article>
       <article class="row">
           <div class="center blog">
               {{ content }}
           </div>
       </article>
       {% include _signup.erb %}
       {% include _footer.erb %}
   </body>
   {% include _scripts.erb %}
</html>
